<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_light_background"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="119dp"
        android:layout_height="86dp"
        app:srcCompat="@drawable/google_logo_png_google_logos_vector_eps_cdr_svg_download_10"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="444dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="248dp"
        android:layout_height="48dp"
        android:layout_marginStart="69dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="128dp"
        android:backgroundTint="#3db58b"
        android:shadowColor="@color/cardview_dark_background"
        android:text="E-mail ile kayıt ol"
        app:icon="@android:drawable/ic_dialog_email"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="165dp"
        android:layout_height="33dp"
        android:layout_marginStart="69dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="152dp"
        android:text="Zaten hesabınız var mı?"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zaten hesabım var"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="587dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="137dp"
        android:layout_height="49dp"
        android:layout_marginEnd="87dp"
        android:layout_marginBottom="37dp"
        android:background="@android:color/transparent"
        android:text="Giriş Yap"
        android:textColor="#3db58b"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="259dp"
        android:layout_height="183dp"
        app:srcCompat="@drawable/fitstorm"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="89dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="casual"
        android:text="Bırak Diyetini Planlasın"
        android:textSize="13dp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="252dp" />

    <Button
        android:id="@+id/button17"
        android:layout_width="248dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/cardview_light_background"
        android:shadowColor="@color/cardview_dark_background"
        android:text="       Google ile Kayıt Ol"
        android:textColor="#353535"
        app:icon="@drawable/google_logo_png_google_logos_vector_eps_cdr_svg_download_10"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="444dp" />

</androidx.constraintlayout.widget.ConstraintLayout>